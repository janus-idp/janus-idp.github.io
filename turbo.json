{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["NEXT_PUBLIC_ANALYTICS_WRITE_KEY"],
  "globalDependencies": ["packages/eslint-config-custom/index.js"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "test:e2e": {
      "dependsOn": ["^build"]
    },
    "test:unit": {},
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
